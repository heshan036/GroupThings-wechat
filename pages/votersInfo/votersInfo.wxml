<!--pages/votersInfo/votersInfo.wxml-->
<view class="votersWrapper">
  <block wx:for="{{voters}}" wx:key="person_nickname">
    <view class="voter">
      <image src="{{item.person_image}}" mode="aspectFill"></image>
      <text class="ml5">{{item.person_nicename}}</text>
    </view>
  </block>
</view>
