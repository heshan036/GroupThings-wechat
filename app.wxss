/**app.wxss**/
page{font-family: 'Microsoft YaHei', '微软雅黑';font-size: 27rpx;background: #f4f7f6;line-height: 1.8}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.flexBox{display: flex;flex-direction: row;align-items: center}
.fr{float: right}
.pr{position: relative}
.tr{text-align: right}
.tc{text-align: center}
.none{display: none}
.mr5{margin-right: 5px}
.ml5{margin-left: 5px}
.c-gray{color: #a0a0a0}
.f12{font-size: 27rpx}
.fb{font-weight: bold}
input,textarea{font-family: 'Microsoft YaHei', '微软雅黑';box-sizing: border-box;}
.inputPlaceholder{color: #a0a0a0}
.prcenter{position: absolute;left: 50%;top: 50%;transform: translate(-50%,-50%)}
.contact{position: fixed;right: 32rpx;bottom:16rpx;}
.secName{color:#a0a0a0;padding: 8px 32rpx 0;display: block}
.btnFooter{position: fixed;left: 0;right: 0;bottom:0;border-radius:0;}
.loading{width:100px;height:20px;margin:16px 0}

@font-face {
	font-family: 'iconfont';
	src: url(data:fonts/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
